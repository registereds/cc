<?php

class myArrayUtil {


    /**
     * Return the value of an array element
     *
     * @param array $arr
     * @param <type> $name
     * @param <type> $default
     * @return <type>
     */
    public static function getValue( $arr,  $name, $default = "") {
        if (isset( $arr[$name])) {
            return $arr[$name];
        }
        else {
            return $default;
        }

        
    }


}